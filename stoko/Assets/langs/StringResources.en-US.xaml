<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <!-- menu home -->
    <system:String x:Key="mHome">Home</system:String>
    <system:String x:Key="setting">Settings</system:String>
    <system:String x:Key="close">Close</system:String>

    <!-- menu help -->
    <system:String x:Key="mHelp">Help</system:String>
    <system:String x:Key="about">About stoko</system:String>

    <!-- tabs -->
    <system:String x:Key="tClient">Clients</system:String>
    <system:String x:Key="tOrder">Orders</system:String>
    <system:String x:Key="tMaterial">Materials</system:String>

    <!-- settings -->
    <system:String x:Key="sSettingClose">Close settings</system:String>
    <system:String x:Key="sGeneral">General</system:String>
    <system:String x:Key="sLang">Language</system:String>
    <system:String x:Key="sDarkMode">Dark mode</system:String>
    <system:String x:Key="sAdvanced">Advanced</system:String>
    <system:String x:Key="sAllowMenu">Show menu</system:String>
</ResourceDictionary>