    <system:String x:Key="mHome">Accueil</system:String>
    <system:String x:Key="setting">Paramètres</system:String>
    <system:String x:Key="close">Fermer</system:String>

    <!-- menu help -->
    <system:String x:Key="mHelp">Aide</system:String>
